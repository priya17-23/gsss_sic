//variables
local-inside fun
global-outside fun
static-keyword:static"it retains its value between multiple function calls"
automatic-keyword:auto,its local
external-keyword:extern,always global
//data types
1.basic/premitive data types
-holds single value
char-1 byte
short int-2 byte
int-4 bytes
long int-8 bytes
float-4 bytes
double-8 bytes
long double-16 bytes
non-premitive /derived data type holds multiple values
1.string
2.array
double pointer-8 bytes
*user defined data types
structure-struct
union
enumeration-enum
//constant
const
*programm 
#include<stdio.h>
int main()
{
	const int a=100;
	a=200;//error
	printf("%d",a);
	return 0;
}
**programm of static**
#include<stdio.h>
void gsss()
{
    //fun defn
    static int a=10,b=20;
    a=a+1,b=b+1;
    printf("%d\t%d\t",a,b);
}
int main()
{
	gsss();//fun call
	gsss();
	gsss();
	return 0;
}
//**external extern variable
#include<stdio.h>
#include"sinchu.h"
int main()
{
  extern int cloud;
  printf("%d",cloud);
}
**programm**
#include<stdio.h>
int main()
{
 int hours;
 float parking_fee;
 printf("enter hours:");
 scanf("%d",&hours);
 if(hours<=2)
 {
     parking_fee=20;
     
 }
 else
 {
     parking_fee=20+(hours-2)*10;
 }
 printf("%.2f",parking_fee);
 
 }
